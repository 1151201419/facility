<%- include('partials/header') %>
<body>
<%- include('partials/banner') %>
<%- include('partials/nav') %>

<!--- THIS IS content--->
<div class='container'>

  <h2 class="title">#<%=qs %>  <%=results.complaintTitle %></h2>

  <form>
    <div class="form-row">
     <div class="col-md-3 mb-3">
       <label>Created Date </label>
       <input type="text" class="form-control" id=""  value="<%=formatDate(results.createDate,1) %>" disabled>
     </div>
     <div class="col-md-4 mb-3">
       <label>Create By</label>
       <input type="text" class="form-control" id=""  value=" <%=results.fullName %>" disabled>
     </div>
     <div class="col-md-2 mb-3">
       <label>Unit No</label>
       <input type="text" class="form-control" id=""  value=" <%=results.address %>" disabled>
     </div>
     <div class="col-md-2 mb-3">
       <label>Status</label>
       <input type="text" class="form-control" id=""  value=" <%=results.statusTitle %>" disabled>
     </div>
   </div>
   <div class="form-row">
     <div class="col-md-3 mb-3">
       <label>Complaint Type</label>
       <input type="text" class="form-control" id="validationDefault03" value=" <%=results.comTypeTitle %>" required>
     </div>

     <div class="col-md-1 mb-3">
       <label>Zip</label>
       <input type="text" class="form-control" id="validationDefault05" placeholder="Zip" required>
     </div>
     <div class="col-md-3 mb-3">
       <label>Assign to</label>
       <select class="selectpicker"  class="form-control"  multiple>
         <% complainttype.forEach(function(complainttype){%>
           <option><%= complainttype.comTypeTitle %></option>
       <% }) %>
    </select>
     </div>
   </div>


   <label>Update Notes</label>
   <textarea   rows="5" class="form-control" id="validationDefault05" placeholder="" required></textarea >
   <label>Closing Notes</label>
   <textarea   rows="5" class="form-control" id="validationDefault05" placeholder="" required></textarea >

   <button class="btn btn-primary" type="submit">Submit</button>








   <!----//---- display picture--------->
       <div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
         <div class="carousel-inner">
           <div class="carousel-item active">
             <img src="/image/pic1.png" class="d-block w-50" alt="...">
           </div>
           <div class="carousel-item" >
             <img src="/image/pic1.png" class="d-block w-50" alt="...">
           </div>
           <div class="carousel-item">
             <img src="/image/pic1.png" class="d-block w-50" alt="...">
           </div>
         </div>
         <a class="carousel-control-prev" href="#carouselExampleInterval" role="button" data-slide="prev">
           <span class="carousel-control-prev-icon" aria-hidden="true"></span>
           <span class="sr-only">Previous</span>
         </a>
         <a class="carousel-control-next" href="#carouselExampleInterval" role="button" data-slide="next">
           <span class="carousel-control-next-icon" aria-hidden="true"></span>
           <span class="sr-only">Next</span>
         </a>
       </div>



  </form>




</div>
<%- include('partials/footer') %>
